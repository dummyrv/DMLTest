select BU_NAME,BATCH_SOURCE_NAME,CUST_TRX_TYPE_NAME,TERM_NAME, TO_CHAR(to_date(TRX_DATE,&apos;YYYY-MM-DD&apos;), &apos;YYYY/MM/DD&apos;) TRX_DATE, TO_CHAR(to_date(GL_DATE,&apos;YYYY-MM-DD&apos;), &apos;YYYY/MM/DD&apos;) GL_DATE, TRX_NUMBER,BILL_CUSTOMER_ACCOUNT_NUMBER,BILL_CUSTOMER_SITE_NUMBER,LINE_TYPE,DESCRIPTION, CURRENCY_CODE, CONVERSION_TYPE,TO_CHAR(to_date(CONVERSION_DATE,&apos;YYYY-MM-DD&apos;), &apos;YYYY/MM/DD&apos;) CONVERSION_DATE,CONVERSION_RATE,AMOUNT,QUANTITY, INTERFACE_LINE_CONTEXT, INTERFACE_LINE_ATTRIBUTE1, INTERFACE_LINE_ATTRIBUTE2, INTERFACE_LINE_ATTRIBUTE3, INTERFACE_LINE_ATTRIBUTE4, INTERFACE_LINE_ATTRIBUTE5, INTERFACE_LINE_ATTRIBUTE6, INTERFACE_LINE_ATTRIBUTE7, INSELECT BU_NAME,
  BATCH_SOURCE_NAME,
  CUST_TRX_TYPE_NAME,
  TERM_NAME,
  TO_CHAR(to_date(TRX_DATE,' 
DD-Mon-YYYY'), 'YYYY/MM/DD') TRX_DATE,
  TO_CHAR(to_date(GL_DATE,'  
DD-Mon-YYYY' ), '  
YYYY/MM/DD' ) GL_DATE,
  TRX_NUMBER,
  BILL_CUSTOMER_ACCOUNT_NUMBER,
  BILL_CUSTOMER_SITE_NUMBER,
  LINE_TYPE,
  DESCRIPTION,
  CURRENCY_CODE,
  CONVERSION_TYPE,
  TO_CHAR(to_date(CONVERSION_DATE, ' 
YYYY-MM-DD' ), '  
YYYY/MM/DD' ) CONVERSION_DATE,
  CONVERSION_RATE,
  AMOUNT,
  QUANTITY,
  INTERFACE_LINE_CONTEXT,
  INTERFACE_LINE_ATTRIBUTE1,
  INTERFACE_LINE_ATTRIBUTE2,
  INTERFACE_LINE_ATTRIBUTE3,
  INTERFACE_LINE_ATTRIBUTE4,
  INTERFACE_LINE_ATTRIBUTE5,
  INTERFACE_LINE_ATTRIBUTE6,
  INTERFACE_LINE_ATTRIBUTE7,
  INTERFACE_LINE_ATTRIBUTE8,
  INTERFACE_LINE_ATTRIBUTE9,
  INTERFACE_LINE_ATTRIBUTE10,
  INTERFACE_LINE_ATTRIBUTE11,
  INTERFACE_LINE_ATTRIBUTE12,
  INTERFACE_LINE_ATTRIBUTE13,
  INTERFACE_LINE_ATTRIBUTE14,
  MEMO_LINE_NAME,
  UOM_CODE,
  FUSION_BATCH_ID
FROM gcaa_dml_ra_interface_lines
WHERE fusion_batch_id       = '1480'
AND interface_process_status='NEW'
AND ROWNUM                 <= 10000
ORDER BY interface_line_id ASC;
TERFACE_LINE_ATTRIBUTE8, INTERFACE_LINE_ATTRIBUTE9, INTERFACE_LINE_ATTRIBUTE10, INTERFACE_LINE_ATTRIBUTE11, INTERFACE_LINE_ATTRIBUTE12, INTERFACE_LINE_ATTRIBUTE13, INTERFACE_LINE_ATTRIBUTE14, MEMO_LINE_NAME, UOM_CODE,FUSION_BATCH_ID from gcaa_dml_ra_interface_lines where fusion_batch_id = &apos;1480&apos; and interface_process_status=&apos;NEW&apos; and ROWNUM <= 10000 order by interface_line_id asc
